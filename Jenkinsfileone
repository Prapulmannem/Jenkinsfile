pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Build - Hello World'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy -Hello World file edited'
            }
        }
        stage('Test') {
            steps {
                echo 'Test - Hello World - file edited'
            }
        }
        stage('Release') {
            steps {
                echo 'Release - Hello World-file edited-again'
            }
        }
    }
}
